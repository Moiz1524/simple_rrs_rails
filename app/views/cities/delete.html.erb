<%= link_to("<< Back to List", cities_path, :class => 'back-link') %>

<div class="subjects delete">
  <h2>Delete City</h2>

  <%= form_for(@city, :method => 'delete') do |c| %>

    <p>Are you sure you want to permanently delete this train?</p>

    <p class="reference-name"><%= c.object.name %></p>

    <div class="form-buttons">
      <%= c.submit("Delete City") %>
    </div>

  <% end %>
</div>